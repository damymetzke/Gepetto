{
  "name": "gepetto",
  "version": "1.0.0",
  "description": "gepetto - SVG animation tool",
  "main": "./out/src/back/main.js",
  "scripts": {
    "start": "node ./node_modules/node-build-util/lib/buildScript.js start",
    "test": "node ./node_modules/node-build-util/lib/buildScript.js test",
    "build-docs": "node ./node_modules/node-build-util/lib/buildScript.js buildDocs",
    "clean": "node ./node_modules/node-build-util/lib/buildScript.js clean",
    "package": "node ./node_modules/node-build-util/lib/buildScript.js package",
    "distribute": "node ./node_modules/node-build-util/lib/buildScript.js distribute",
    "old:compile-sass": "node ./node_scripts/compile-sass.js",
    "old:typescript-front": "./node_modules/.bin/tsc -p ./config/front.tsconfig.json",
    "old:typescript-back": "./node_modules/.bin/tsc -p ./config/back.tsconfig.json",
    "old:start": "electron .",
    "old:typescript-test": "./node_modules/.bin/tsc -p ./config/test.tsconfig.json",
    "old:test": "jest",
    "old:build-typedoc": "node ./node_scripts/wrap_command.js \".\\node_modules\\.bin\\typedoc ./intermediate/typedoc_src/ --out {build_config:directoryMaps.typedoc.to} --tsconfig ./config/tsconfig.json --exclude ./src/front/core --exclude ./src/back/core --exclude ./src/test/core\"",
    "old:prepare-typedoc": "node ./node_scripts/move-typedoc-source.js",
    "old:clean": "node ./node_scripts/clean.js",
    "old:pack": "./node_modules/.bin/electron-builder --dir",
    "old:dist": "./node_modules/.bin/electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damymetzke/Gepetto"
  },
  "author": "Damy Metzke",
  "license": "UNLICENCED",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/types": "^7.9.6",
    "@types/jest": "^25.2.3",
    "del": "^5.1.0",
    "electron": "^8.2.4",
    "electron-builder": "^22.7.0",
    "jest": "^25.2.7",
    "jsdoc": "^3.6.3",
    "markdown": "^0.5.0",
    "marked": "^1.1.0",
    "node-build-util": "^0.1.0",
    "npm-sass": "^2.3.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "build": {
    "appId": "com.damymetzke.gepetto",
    "productName": "Gepetto",
    "copyright": "Copyright Â© 2020 Damy Metzke",
    "directories": {
      "buildResources": "./",
      "output": "./distribution"
    },
    "files": [
      "./out/**/*"
    ],
    "fileAssociations": [
      {
        "ext": "gpp",
        "name": "Gepetto Project File",
        "role": "Editor"
      },
      {
        "ext": "gpp.pak",
        "name": "Packaged Gepetto File",
        "role": "Viewer"
      }
    ]
  }
}