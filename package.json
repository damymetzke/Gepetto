{
  "name": "gepetto",
  "version": "1.0.0",
  "description": "gepetto - SVG animation tool",
  "main": "./out/src/back/main.js",
  "private": true,
  "scripts": {
    "start": "b-script start",
    "test": "b-script test",
    "lint": "eslint --max-warnings 0 src/**",
    "compile-app": "b-script compileApp",
    "build-docs": "b-script buildDocs",
    "clean": "b-script clean",
    "package": "b-script package",
    "distribute": "b-script distribute",
    "old:prepare-typedoc": "node ./node_scripts/move-typedoc-source.js",
    "old:clean": "node ./node_scripts/clean.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damymetzke/Gepetto"
  },
  "author": "Damy Metzke",
  "license": "UNLICENCED",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/types": "^7.11.5",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "del": "^5.1.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.6",
    "marked": "^1.2.0",
    "node-build-util": "^0.2.0",
    "npm-sass": "^2.3.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "xml-js": "^1.6.11"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "build": {
    "appId": "com.damymetzke.gepetto",
    "productName": "Gepetto",
    "copyright": "Copyright Â© 2020 Damy Metzke",
    "directories": {
      "buildResources": "./",
      "output": "./distribution"
    },
    "files": [
      "./out/**/*"
    ],
    "fileAssociations": [
      {
        "ext": "gpp",
        "name": "Gepetto Project File",
        "role": "Editor"
      },
      {
        "ext": "gpp.pak",
        "name": "Packaged Gepetto File",
        "role": "Viewer"
      }
    ]
  }
}
