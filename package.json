{
  "name": "gepetto",
  "version": "1.0.0",
  "description": "gepetto - SVG animation tool",
  "main": "./out/src/back/main.js",
  "scripts": {
    "start": "b-script start",
    "test": "b-script test",
    "build-docs": "b-script buildDocs",
    "clean": "b-script clean",
    "package": "b-script package",
    "distribute": "b-script distribute",
    "old:build-typedoc": "node ./node_scripts/wrap_command.js \".\\node_modules\\.bin\\typedoc ./intermediate/typedoc_src/ --out {build_config:directoryMaps.typedoc.to} --tsconfig ./config/tsconfig.json --exclude ./src/front/core --exclude ./src/back/core --exclude ./src/test/core\"",
    "old:prepare-typedoc": "node ./node_scripts/move-typedoc-source.js",
    "old:clean": "node ./node_scripts/clean.js",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damymetzke/Gepetto"
  },
  "author": "Damy Metzke",
  "license": "UNLICENCED",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/types": "^7.11.5",
    "@types/jest": "^25.2.3",
    "del": "^5.1.0",
    "electron": "^8.5.0",
    "electron-builder": "^22.8.0",
    "jest": "^25.5.4",
    "jsdoc": "^3.6.5",
    "markdown": "^0.5.0",
    "marked": "^1.1.1",
    "node-build-util": "^0.2.0",
    "npm-sass": "^2.3.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "build": {
    "appId": "com.damymetzke.gepetto",
    "productName": "Gepetto",
    "copyright": "Copyright Â© 2020 Damy Metzke",
    "directories": {
      "buildResources": "./",
      "output": "./distribution"
    },
    "files": [
      "./out/**/*"
    ],
    "fileAssociations": [
      {
        "ext": "gpp",
        "name": "Gepetto Project File",
        "role": "Editor"
      },
      {
        "ext": "gpp.pak",
        "name": "Packaged Gepetto File",
        "role": "Viewer"
      }
    ]
  }
}